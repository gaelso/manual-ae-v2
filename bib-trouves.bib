@MASTERSTHESIS{pichancourt03,
  author = {Pichancourt, J. B.},
  title = {Un mod\`ele d\'ecrivant la dynamique d?une population d'\textit{Abax
	ater} dans diff\'erents paysages bocagers bretons},
  school = {Universit\'e Grenoble 1},
  year = {2003},
  type = {M\'emoire de {DEA}},
  address = {La Tronche, France},
  abstract = {Nous pr\'esentons un mod\`ele multir\'egional de Leslie pour la dynamique
	de population d?\textit{Abax ater} (7 classes d?\^age) \'evoluant
	dans diff\'erents types de paysages agricoles (quatre types d?habitats
	: les bois, les chemins creux, les haies et la matrice agricole),
	Ce mod\`ele couple un sous mod\`ele de dispersion inter-habitat avec
	un sous mod\`ele d\'emographique de Leslie. Le mod\`ele int\`egre
	en plus la possibilit\'e d?adapter la vitesse de dispersion des individus
	en fonction de la taille des taches d?habitats. Nous \'etudions comment
	l?am\'enagement du territoire (en modifiant les proportions des habitats
	et/ou en modifiant la taille des habitats) peut influencer la dynamique
	globale de la population. Nous montrons que les bois et les chemins
	creux sont favorables pour la dynamique de la population, tandis
	que les haies et la matrice agricole ne le sont pas. Lorsqu?il n?y
	a plus de chemins creux, au dessus de 5% de bois la population peut
	cro\^{\i}tre dans le paysage. En dessous de 5 %, une destruction
	de 1% de bois doit \^etre compens\'ee par la construction de trois
	fois plus de chemins creux. La destruction de bois et de chemins
	creux a un effet n\'egatif essentiellement sur les classes d?\^age
	larvaires.},
  keywords = {Leslie multir\'egional / diff\'erentes \'echelles de temps / m\'ethode
	d?agr\'egation / paysage bocager / Abax ater},
  pages = {32},
  source = {Avner}
}

