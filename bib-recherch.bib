@MASTERSTHESIS{couralet04,
  author = {Couralet, Camille},
  title = {Growth and population dynamics of \textit{Juniperus procera} in an
	{E}thiopian dry afromontane forest: dendrochronology and matrix model:
	a study in the {A}daba-{D}odola forest priority area},
  school = {Wageningen University},
  year = {2004},
  address = {Wageningen, The Netherlands},
  abstract = {Highland forests in Ethiopia are essential socially, economically,
	ecologically and for their cultural and symbolic value. However they
	suffered degradation for thousands of years, with an exponential
	increase since the middle of the XXth century. Nowadays there is
	need and will for sustainable management of the remaining woodlands
	and for restoration of the degraded ones. Dominant indigenous species
	of these dry afromontane forests, like Juniperus procera, deserve
	paramount attention for this purpose. We studied the growth and population
	dynamics of this species in the Adaba-Dodola Forest Priority Area,
	south of the country. From an inventory we assessed the present status
	of Juniperus procera in the Adaba-Dodola forest. Then we evaluated
	the dendrochronological potential of the species. Eventually, from
	these two investigations we built a matrix model, to project the
	evolution of the juniper population into the future and gain indications
	for a sustainable forest use. We found two juniper population types
	in the study area, without being able to explain this difference.
	Juniperus procera forms annual growth layers in response to precipitation.
	Apparently it also grows in response to intra-annual precipitation
	variations and expresses it anatomically by variations in cell-wall
	density. Matrix-model output was not influenced by small methodological
	changes, but its interpretation remains delicate. This limitation
	can be avoided by a qualitative and quantitative improvement of the
	input data. Nevertheless, for wood supply we recommend the use of
	big and fully-grown trees from the second population type that we
	pointed out, instead of adult sound junipers.},
  keywords = {Juniperus procera; Population dynamics; Dendrochronology; Growth;
	Ethiopia; modele matriciel},
  pages = {68}
}

