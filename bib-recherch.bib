  volume = {16},
  pages = {507-520},
  number = {4},
  abstract = {The temporal stability of estimates of demographic behaviour for a
	sample population of the tropical rainforest gymnosperm Araucaria
	cunninghamii was analysed using a stage-classified (Lefkovitch) matrix
	model. Previously published life-table analyses, which used only
